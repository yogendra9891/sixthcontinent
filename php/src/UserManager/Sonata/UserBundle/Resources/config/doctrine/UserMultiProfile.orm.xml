<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="UserManager\Sonata\UserBundle\Entity\UserMultiProfileRepository" name="UserManager\Sonata\UserBundle\Entity\UserMultiProfile">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="userId" type="integer" column="user_id"/>
    <field name="email" type="string" column="email" length="255"/>
    <field name="firstName" type="string" column="first_name" length="255"/>
    <field name="lastName" type="string" column="last_name" length="255"/>
    <field name="gender" type="string" column="gender" length="255"/>
    <field name="birthDate" type="date" column="birth_date"/>
    <field name="phone" type="string" column="phone" length="255"/>
    <field name="country" type="string" column="country" length="255"/>
    <field name="relationship" type="string" column="relationship" length="100"/>
    <field name="hobbies" type="string" column="hobbies" length="500"/>
    <field name="street" type="text" column="street"/>
    <field name="profileType" type="integer" column="profile_type"/>
    <field name="isActive" type="boolean" column="is_active"/>
    <field name="createdAt" type="datetime" column="created_at"/>
    <field name="updatedAt" type="datetime" column="updated_at"/>
    <field name="profileSetting" type="integer" column="profile_setting"/>
    
  </entity>
</doctrine-mapping>
